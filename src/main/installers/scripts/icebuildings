#!/bin/bash

CACHE_DIR="${HOME}/.cache/icebuildings"
mkdir -p "${CACHE_DIR}"
pushd "${CACHE_DIR}"
for i in /usr/lib/icebuildings/* ; do
	basename="$(basename ${i})"
	if [ ! -f "${basename}" -a ! -L "${basename}" ]; then
		ln -s "${i}" . 2>/dev/null
	fi
done
java -jar /usr/lib/icebuildings/getdown.jar . 